{
    "name": "smartness/translation-client",
    "description": "Laravel package to pull translations from SmartPMS Translation Manager",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "translations",
        "i18n",
        "smartpms",
        "localization"
    ],
    "authors": [
        {
            "name": "Smartness",
            "email": "dev@smartpricing.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Smartness\\TranslationClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smartness\\TranslationClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smartness\\TranslationClient\\TranslationClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
